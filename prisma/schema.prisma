generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             String  @id @default(uuid())
  email          String  @unique
  senha          String
  tipo_chave_pix String
  chave_pix      String
  recoverToken   String?
}
